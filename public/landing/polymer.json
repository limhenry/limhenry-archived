{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-home.html",
    "src/my-awards.html",
    "src/my-projects.html",
    "src/my-talks.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json",
    "data/**"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ],
  "builds": [{
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addPushManifest": true,
    "insertPrefetchLinks": true
  }]
}